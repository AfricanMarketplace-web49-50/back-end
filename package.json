    "helmet": "5.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "0.95.7",
    "knex-cleaner": "1.3.1",
    "pg": "8.7.1",
    "pgtools": "^0.3.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.8.0",
