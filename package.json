  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "14.3.0",
    "express": "4.17.2",
    "helmet": "5.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "knex": "0.20.4",
    "knex-cleaner": "1.3.1",
    "pg": "7.15.1",
    "sqlite3": "^4.1.1"
